{{outlet}}
<article class="container">
<h1>Conoce este proyecto</h1>

<section class="details_header row">
    <article class="col s12 m6">
        <div class="video-container">
        
        {{#unless model.video}}
        <iframe width="560" height="315" src="https://www.youtube.com/embed/iJidOOmAl8c" frameborder="0" allowfullscreen></iframe>
        {{else}}
        <iframe width="560" height="315" src="{{model.video}}" frameborder="0" allowfullscreen></iframe>
        
        {{/unless}}

        
        </div>
    </article>
    <article class="col s12 m6">
        <h5>{{model.titulo}}</h5>
        <p>
            Pequeña descripción
            Lorem ipsum dolor sit amet, consectetur adipisicing elit. 
            Perspiciatis, laboriosam, delectus? Accusamus aut odio at 
            cum dolor itaque mollitia est incidunt ipsa fugiat, expedita 
            veritatis neque rerum officiis ad, quae.
        </p>
        <div class="chip">
            <img class="responsive-img" src="http://materializecss.com/images/yuna.jpg" alt="Contact Person">
            Jane Doe
        </div>
        <div class="chip">
            Cultura
        </div>
        <div class="votes_progress_container">
            <strong>Votos</strong>
            <div class="progress">
                <div class="determinate green" style="width: 70%"></div>
            </div>
        </div>
        <button {{action 'votar' model}} class="waves-effect waves-light btn col m4 green">Votar</button>

        {{#if session.isAuthenticated}}

        {{#link-to 'projects.edit' model}}
        <button class="waves-effect waves-light btn col m4 orange">Editar</button>
        {{/link-to}}



        {{/if}}
{{!-- Numero de votos --}}
        <i class="fa fa-heart-o icoon fa-2x" aria-hidden="true">
            <span>{{model.votos_number}}</span></i>
{{!-- Numero de votos --}}
    </article>
</section>
<section class="details_main row">
    <article class="col m8">
        <p>
            {{model.desc}}
        </p>
        <div>
